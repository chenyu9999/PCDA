<template name="coupon-template">
    <view class="alert-box">
        <view class="alert-background">
            <view class="alert-background-placeholder" style="bottom: 890rpx;"></view>
            <view class="alert-conntent-box alert-service-guarantee" style="height: 890rpx;bottom: 0;">
                <view class="alert-content-title" style="border-bottom:1rpx solid #DCDCDC;">
                    <view catchtap="onhidePopup" class="alert-close sprites-icon1"></view>
                    <text class="color-333 font32 tc">使用优惠券</text>
                </view>
                <view class="selected-box">
                    <view class="coupon-title font26 color-666">
                        <view catchtap="switchShowType" class="{{couponData.showType===1?'select':''}}" data-showType="1">可用优惠券({{couponData.availableNum>99?'99+':couponData.availableNum}})</view>
                        <view catchtap="switchShowType" class="{{couponData.showType===2?'select':''}}" data-showType="2">不可用优惠券({{couponData.unAvailableNum>99?'99+':couponData.unAvailableNum}})</view>
                    </view>
                    <view class="coupon-status background-f6f6f6 font24 color-666" wx:if="{{couponData.coupon1&&couponData.showType===1}}">
                        <text>{{couponData.selectIndex===null?'请选择优惠券':'已选中优惠券，共可抵扣'}}</text>
                        <text wx:if="{{couponData.selectIndex!==null}}">{{'￥'+filter.float1(couponData.coupon1[couponData.selectIndex].price)}}</text>
                    </view>
                    <block wx:if="{{couponData.showType===1}}">
                        <view class="selected-content background-f6f6f6">
                            <scroll-view scrollY bindscrolltolower="couponReachBottom" class="coupon-list1" wx:if="{{couponData.coupon1}}">
                                <view catchtap="selectCoupon" class="list" data-index="{{index}}" wx:for="{{couponData.coupon1}}">
                                    <view class="list_box">
                                        <view class="list_ce">
                                            <image class="list_img" src="../../../img/j_zhi.png" wx:if="{{index!==couponData.selectIndex&&couponData.selectIndex!==null}}"></image>
                                            <image class="list_img" src="http://tu.duia.com/mp/p_tong.png" wx:else></image>
                                            <view class="list_cess">
                                                <view class="list_cess_box">
                                                    <view>￥ <text class="list_cess_size">{{item.price}}</text>
                                                    </view>
                                                    <view class="list_cess_b">无使用门槛</view>
                                                </view>
                                            </view>
                                        </view>
                                        <view class="list_right">
                                            <view class="list_right_top">
                                                <text class="list_label gray" wx:if="{{index!==couponData.selectIndex&&couponData.selectIndex!==null}}">普通券</text>
                                                <text class="list_label green" wx:else>普通券</text>
                                                <text class="list_title">限购"{{item.skuName}}"学科下{{item.comName?'"'+item.comName+'"':item.courseType?'专题课':item.courseType===-1?'全部':'系统班'}}课程</text>
                                            </view>
                                            <view class="list_right_bottom">
                                                <view class="list_right_bottom_time">{{item.startDate}} - {{item.endDate}}</view>
                                                <view class="list_right_bottom_btn1">
                                                    <image class="list_right_bottom_bimg" src="../../../img/xuan.png" wx:if="{{index===couponData.selectIndex}}"></image>
                                                    <image class="list_right_bottom_bimg" src="../../../img/wei.png" wx:else></image>
                                                </view>
                                            </view>
                                        </view>
                                    </view>
                                </view>
                            </scroll-view>
                            <view class="wu_data" wx:else>
                                <image class="wu_data_img" src="http://tu.duia.com/mp/noSeat.png"></image>
                                <view class="wu_data_text">您暂时没有可用的优惠券哦~</view>
                            </view>
                        </view>
                        <view catchtap="onSelectCoupon" class="selected-foot font34 color-fff"> 确定 </view>
                    </block>
                    <view class="selected-content background-f6f6f6" wx:if="{{couponData.showType===2}}">
                        <scroll-view scrollY bindscrolltolower="couponReachBottom" class="coupon-list2" wx:if="{{couponData.coupon2}}">
                            <view catchtap="onclick" class="list" data-index="{{index}}" wx:for="{{couponData.coupon2}}">
                                <view class="list_box">
                                    <view class="list_ce">
                                        <image class="list_img" src="http://tu.duia.com/mp/p_tong.png" wx:if="{{item.type=='pt'}}"></image>
                                        <image class="list_img" src="http://tu.duia.com/mp/t_quan.png" wx:if="{{item.type=='yhtq'}}"></image>
                                        <view class="list_cess">
                                            <view class="list_cess_box">
                                                <view>￥ <text class="list_cess_size">{{item.price}}</text>
                                                </view>
                                                <view class="list_cess_b">无使用门槛</view>
                                            </view>
                                        </view>
                                    </view>
                                    <view class="list_right">
                                        <view class="list_right_top">
                                            <text class="list_label green" wx:if="{{item.type=='pt'}}">普通券</text>
                                            <text class="list_label orange" wx:if="{{item.type=='yhtq'}}">特权券</text>
                                            <text class="list_title">限购"{{item.skuName}}"学科下{{item.comName?'"'+item.comName+'"':item.courseType?'专题课':item.courseType===-1?'全部':'系统班'}}课程</text>
                                        </view>
                                        <view class="list_right_bottom">
                                            <view class="list_right_bottom_time">{{item.startDate}} - {{item.endDate}}</view>
                                            <view class="list_right_bottom_btn" wx:if="{{item.type=='pt'}}">立即使用</view>
                                            <view class="list_right_bottom_btns" wx:if="{{item.type=='yhtq'}}">使用说明</view>
                                        </view>
                                    </view>
                                </view>
                                <view class="condition">
                                    <text class="condition_text" wx:if="{{item.type=='pt'}}">当前结算订单中没有符合条件的课程</text>
                                    <text wx:else>特权券不支持直接使用</text>
                                </view>
                            </view>
                        </scroll-view>
                        <view class="wu_data" wx:else>
                            <image class="wu_data_img" src="http://tu.duia.com/mp/noSeat.png"></image>
                            <view class="wu_data_text" wx:if="{{item.type=='yhtq'}}">您暂时没有不可用的优惠券哦~</view>
                        </view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</template>
<template name="promo-code-template">
