<template name="reminder-discount-template">
    <view class="content_list">
        <view class="list" wx:for="{{list}}">
            <view class="list_box">
                <view bindtap="employ" class="list_box_cen" data-index="{{index}}" wx:if="{{item.discountType=='pt'&&nav_type==2}}"></view>
                <view bindtap="state" class="list_box_cen" data-index="{{index}}" wx:if="{{item.discountType=='yhtq'&&nav_type==2}}"></view>
                <view bindtap="select" class="list_box_cen" wx:if="{{item.wode==2&&!item.guoqi}}"></view>
                <view class="list_ce">
                    <image class="list_img" src="http://tu.duia.com/mp/p_tong.png" wx:if="{{item.discountType=='pt'&&nav_type==2}}"></image>
                    <image class="list_img" src="http://tu.duia.com/mp/t_quan.png" wx:if="{{item.discountType=='yhtq'&&nav_type==2}}"></image>
                    <image class="list_img" src="../../img/j_zhi.png" wx:if="{{nav_type==3}}"></image>
                    <view class="list_cess">
                        <view class="list_cess_box">
                            <view>￥ <text class="list_cess_size">{{item.discount}}</text>
                            </view>
                            <view class="list_cess_b">无使用门槛</view>
                        </view>
                    </view>
                </view>
                <view class="list_right">
                    <view class="list_right_top">
                        <text class="list_label green" wx:if="{{item.discountType=='pt'&&nav_type==2}}">普通券</text>
                        <text class="list_label orange" wx:if="{{item.discountType=='yhtq'&&nav_type==2}}">特权券</text>
                        <text class="list_label gray" wx:if="{{nav_type==1||nav_type==3}}">{{item.discountType=='yhtq'?'特权券':'普通券'}}</text>
                        <text class="list_title">限购"{{item.skuName}}"学科下{{item.commodName?'"'+item.commodName+'"':item.courseType===1?'专题课':item.courseType===-1?'全部':'系统班'}}课程</text>
                    </view>
                    <view class="list_right_bottom">
                        <view class="list_right_bottom_time">{{item.startDateStr}} - {{item.endDateStr}}</view>
                        <view class="list_right_bottom_btn" wx:if="{{item.discountType=='pt'&&nav_type==2}}">立即使用</view>
                        <view class="list_right_bottom_btns" wx:if="{{item.discountType=='yhtq'&&nav_type==2}}">使用说明</view>
                        <view class="list_right_bottom_btn1" wx:if="{{item.wode==2&&!item.guoqi}}">
                            <image class="list_right_bottom_bimg" src="../../img/{{item.xuanze?'xuan.png':'wei.png'}}"></image>
                        </view>
                    </view>
                    <image class="list_right_past" src="http://tu.duia.com/mp/yishiyong.png" wx:if="{{nav_type==1}}"></image>
                    <image class="list_right_past" src="http://tu.duia.com/mp/y_guoqi.png" wx:if="{{nav_type==3}}"></image>
                </view>
            </view>
            <view class="condition" wx:if="{{queren}}">
                <text class="condition_text" wx:if="{{item.type==1}}">当前结算订单中没有符合条件的课程</text>
            </view>
        </view>
    </view>
</template>
<template name="reminder-agreement-template">
    <block wx:for="{{list}}" wx:key="key">
        <view class="reminder-item">
            <view class="reminder-box title">
                <view class="reminder-icon">
                    <view class="alert-serviceGuarantee-icon sprites-icon1"></view>
                </view>
                <view class="reminder-content text-overflow-dotted-1line color-333 font28">{{text}}</view>
            </view>
            <view class="reminder-box class-name" wx:if="{{item.comName.length>0}}">
                <view class="reminder-icon"></view>
                <view class="reminder-content color-999 font24">包含此协议课程：<text wx:for="{{item.comName}}" wx:for-index="classindex" wx:for-item="classitem" wx:key="classkey">{{'“'+classitem+'”'}}</text>
                </view>
            </view>
            <view class="reminder-box">
                <viem class="reminder-icon"></viem>
                <viem class="reminder-content">
                    <view class="color-666 font28">
                        <template is="wxParse" data="{{wxParseData:item.content.nodes}}"></template>
                    </view>
                </viem>
            </view>
        </view>
        <view class="px-half"></view>
    </block>
</template>
<template name="reminder-template">
    <view class="alert-box">
        <view class="alert-background">
            <view class="alert-background-placeholder" style="bottom: 890rpx;"></view>
            <view class="alert-conntent-box alert-service-guarantee" style="height: 890rpx;bottom: 0;">
                <view class="alert-content-title" style="border-bottom:1rpx solid #DCDCDC;">
                    <view catchtap="onhidePopup" class="alert-close sprites-icon1"></view>
                    <text>购课温馨提示</text>
                </view>
                <view class="selected-box">
                    <view class="selected-content {{reminderData&&reminderData.length===1?'hidden':''}}">
                        <block wx:if="{{reminderData['0'].tips.length>0}}">
                            <view class="reminder-item">
                                <view class="reminder-box title">
                                    <view class="reminder-icon">
                                        <view class="alert-specialService-icon" style="background-position:-97rpx -136rpx"></view>
                                    </view>
                                    <view class="reminder-content color-333 font28 text-overflow-dotted-1line">班级信息</view>
                                </view>
                                <block wx:for="{{reminderData['0'].tips}}" wx:key="key">
                                    <view class="reminder-box name">
                                        <view class="reminder-icon">
                                            <image class="reminder-icom2" src="../../img/termList2.png"></image>
                                        </view>
                                        <view class="reminder-content text-overflow-dotted-1line color-666 font28">{{item.name}}</view>
                                    </view>
                                    <view class="reminder-box time">
                                        <view class="reminder-icon">
                                            <view class="reminder-icon1"></view>
                                        </view>
                                        <view class="reminder-content color-999 font24" wx:if="{{item.guaType===1||item.guaType===0||item.guaType===2&&filter.getJSONItem(item.secondData,'gua')==0}}"> {{item.classEnd?'本期课程'+tools.timeFormat(item.classEnd)+'结课':''}} {{item.deadline?tools.timeFormat(item.deadline)?'/质保期至'+tools.timeFormat(item.deadline):'':''}} </view>
                                        <view class="reminder-content color-999 font24" wx:if="{{item.guaType===2&&filter.getJSONItem(item.secondData,'gua')==1}}"> {{item.classEnd?'本期课程'+tools.timeFormat(item.classEnd)+'结课/':''}}/质保期至{{item.stopTime?tools.timeFormat(item.stopTime):''}}，到期符合条件可延期至{{item.deadline?tools.timeFormat(item.deadline):''}} </view>
                                    </view>
                                </block>
                            </view>
                            <view class="px-half"></view>
                        </block>
                        <block wx:if="{{reminderData['99'].tips.length>0}}">
                            <view class="reminder-item">
                                <view class="reminder-box title">
                                    <view class="reminder-icon">
                                        <view class="alert-specialService-icon alert-specialService-icon1" style="background:none;">
                                            <image src="../../../img/baohu.png" style="width:100%;height:100%;"></image>
                                        </view>
                                    </view>
                                    <view class="reminder-content text-overflow-dotted-1line color-333 font28">账号保护</view>
                                </view>
                                <view class="reminder-box class-name" wx:if="{{reminderData['99'].tips.length>0}}">
                                    <view class="reminder-icon"></view>
                                    <view class="reminder-content color-999 font24"> 包含账号保护课程：<text wx:for="{{reminderData['99'].tips}}" wx:for-index="classindex" wx:for-item="classitem" wx:key="classkey"> {{'“'+classitem.comName[0]+'”'}}</text>
                                    </view>
                                </view>
                                <view class="reminder-box">
                                    <viem class="reminder-icon"></viem>
                                    <viem class="reminder-content">
                                        <view class="color-666 font28">质保期内免费开启账号保护，异常登录时将启用人脸识别功能保护账号安全，账号保护期间仅供本人使用</view>
                                    </viem>
                                </view>
                            </view>
                            <view class="px-half"></view>
                        </block>
                        <template is="reminder-agreement-template" data="{{list:reminderData['3'].tips,text:'质保期协议'}}" wx:if="{{reminderData['3'].tips.length>0}}"></template>
                        <template is="reminder-agreement-template" data="{{list:reminderData['4'].tips,text:'退款协议'}}" wx:if="{{reminderData['4'].tips.length>0}}"></template>
                        <template is="reminder-agreement-template" data="{{list:reminderData['6'].tips,text:'七天价保'}}" wx:if="{{reminderData['6'].tips.length>0}}"></template>
                        <template is="reminder-agreement-template" data="{{list:reminderData['2'].tips,text:'课程协议'}}" wx:if="{{reminderData['2'].tips.length>0}}"></template>
                    </view>
                    <view catchtap="onConfirmPopup" class="selected-foot font34 color-fff">我同意</view>
                </view>
            </view>
        </view>
    </view>
</template>
